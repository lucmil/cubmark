#ifndef CUBMARK_CONFIG_H
#define CUBMARK_CONFIG_H

/* Program version */
#cmakedefine CUBMARK_VERSION "@CUBMARK_VERSION@"

/* Disable assertion code (default assertions enabled) */
#cmakedefine CUBMARK_NDEBUG

/* Enable use of exceptions */
#cmakedefine CUBMARK_USE_EXCEPTIONS

/* Enable GSL support */
#cmakedefine CUBMARK_HAS_GSL

/* Enable r2d2lri support */
#cmakedefine CUBMARK_HAS_R2D2LRI

/* Enable HIntLib support */
#cmakedefine CUBMARK_HAS_HINTLIB

/* Enable DCUHRE support */
#cmakedefine CUBMARK_HAS_DCUHRE

/* Enable Cubpack++ support */
#cmakedefine CUBMARK_HAS_CUBPACK

/* std::unique_ptr support */
#cmakedefine HAVE_UNIQUE_PTR

#endif//CUBMARK_CONFIG_H
